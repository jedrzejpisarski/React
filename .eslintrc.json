{
  "rules": {
      "indent": [
          1,
          2
      ],
      "quotes": [
          2,
          "double"
      ],
      "linebreak-style": [
          2,
          "windows"
      ],
      "semi": [
          2,
          "always"
      ]
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
  },
  "plugins": [
      "react",
      "redux"
  ]
}